        <servlet-name>LocationAjax</servlet-name>
        <url-pattern>/LocationAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SizeCheckAjax</servlet-name>
        <url-pattern>/SizeCheckAjax</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
