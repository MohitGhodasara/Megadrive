        <servlet-class>MegaDrive.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>MegaDrive.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>MegaDrive.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateAjax</servlet-name>
        <servlet-class>MegaDrive.CreateAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAjax</servlet-name>
        <servlet-class>MegaDrive.DeleteAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>MegaDrive.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreviousAjax</servlet-name>
