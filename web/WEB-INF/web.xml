        <servlet-class>MegaDrive.GetInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>MegaDrive.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SizeServlet</servlet-name>
        <servlet-class>MegaDrive.SizeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LocationAjax</servlet-name>
        <servlet-class>MegaDrive.LocationAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SizeCheckAjax</servlet-name>
        <servlet-class>MegaDrive.SizeCheckAjax</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateAjax</servlet-name>
        <url-pattern>/CreateAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAjax</servlet-name>
        <url-pattern>/DeleteAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreviousAjax</servlet-name>
        <url-pattern>/PreviousAjax</url-pattern>
    </servlet-mapping>
